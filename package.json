{
  "name": "abortable-promise-cache",
  "version": "1.1.0",
  "description": "add AbortController support to a cache of async requests",
  "license": "MIT",
  "repository": "rbuels/abortable-promise-cache",
  "main": "dist/index.js",
  "author": {
    "name": "Robert Buels",
    "email": "rbuels@gmail.com",
    "url": "https://github.com/rbuels"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "tsc --noEmit",
    "docs": "documentation readme --shallow src/AbortablePromiseCache.js --section=API",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc",
    "build:js": "babel src -d dist",
    "watch": "npm-watch",
    "prepublishOnly": "npm run lint && npm test && npm run build",
    "postpublish": "git push origin master --follow-tags"
  },
  "watch": {
    "test": "{src,test}/*.js",
    "lint": "{src,test}/*.js",
    "build": "src"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "abortcontroller-polyfill": "^1.2.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/node": "^12.0.12",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^24.0.0",
    "documentation": "^6.1.0",
    "eslint": "^5.0.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-prettier": "^2.6.0",
    "jest": "^24.0.0",
    "lodash": "^4.17.10",
    "npm-watch": "^0.5.0",
    "opn-cli": "^3.1.0",
    "prettier": "^1.18.2",
    "quick-lru": "^2.0.0",
    "rimraf": "^2.6.2",
    "typescript": "^3.5.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
